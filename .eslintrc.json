{
  "env": {
    "es6": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-undef": "warn",
    "no-unused-vars": "warn",
    "react/no-unescaped-entities": "off"
  }
}
